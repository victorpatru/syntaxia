{
  "permissions": {
    "deny": ["Shell(git push)", "Shell(gh pr create)", "Write(**)"],
    "allow": [
      "Shell(gh pr view)",
      "Shell(gh pr diff)",
      "Shell(gh pr comment)",
      "Shell(gh api)",
      "Read(**/*.ts)",
      "Read(**/*.tsx)",
      "Read(**/*.js)",
      "Read(**/*.jsx)",
      "Read(.cursor/rules/**)"
    ]
  }
}
